{
  "name": "psd-vanilla-js",
  "version": "0.1.39",
  "description": "A robust vanilla JavaScript library designed to parse and handle PSD",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "asbuild:debug": "asc assembly/index.ts --target debug",
    "asbuild:release": "asc assembly/index.ts --target release",
    "asbuild": "npm run asbuild:debug && npm run asbuild:release",
    "test": "node tests",
    "start": "npx serve ."
  },
  "keywords": [
    "psd",
    "JavaScript",
    "vanilla",
    "sellersmith",
    "parser",
    "photoshop",
    "adobe",
    "reader"
  ],
  "files": [
    "dist",
    "build"
  ],
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:sellersmith/psd-vanilla-js.git"
  },
  "devDependencies": {
    "assemblyscript": "^0.27.27",
    "vite": "^5.2.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.27.27"
  }
}
